FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1.46E-07 0 0);

boundaryField
{
    outlet
    {
        type            zeroGradient;
        //freestreamValue	uniform (0.04 0 0);

    }

    inlet
    {
        type            fixedValue;
	value		$internalField;
    }

    rotWall
    {
	//type		rotatingWallVelocity;
	//origin		(0 0 0);
	//axis		(0 0 1);
	//omega		0;
	//value		uniform (0 0 0);
	//type		fixedValue;
	//value		uniform (0 0 0);
	type 		noSlip;
    }


    frontAndBackPlanes
    {
        type            empty;
    }

    /*MRFinner1
    {
        type            cyclicAMI;
    }


    MRFouter1
    {
        type            cyclicAMI;
    }

    MRFinner2
    {
        type            cyclicAMI;
    }


    MRFouter2
    {
        type            cyclicAMI;
    }*/
    
    MRFOuter1
    {
        type            cyclicAMI;
        value		$internalField;
    }

    MRFOuter2
    {
        type            cyclicAMI;
        value		$internalField;
    }

    
   upper_wall
    {
    	type		slip;
    }
    
    lower_wall
    {
    	type		slip;
    }

}

