FoamFile
{
    format      ascii;
    class       dictionary;
    object      MRFProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

zone1
{
    cellZone    rotFluid;
    active      yes;
    

    // Fixed patches (by default they 'move' with the MRF zone)
    //nonRotatingPatches ();
    nonRotatingPatches (MRFOuter1 MRFOuter2);

    origin    (0 0 0);
    axis      (0 0 1);
    omega     -2.92E-07;   //15000 deg/s or 2500 rpm
}

